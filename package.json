    "npm": "6.14.8",
    "yarn": "1.22.5"
  },
  "license": "Apache-2.0",
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules",
      "test"
    ],
    "watch": [
      "server",
      "cron",
      "mailer",
      "shared"
    ],
    "exec": "npm run dev:exec",
    "ext": "ts"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
