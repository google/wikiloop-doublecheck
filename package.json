{
  "name": "wikiloop-doublecheck",
  "version": "4.1.0",
  "description": "A web app for Wikipedians to fight vandalism together.",
  "author": "Zainan Victor Zhou <zzn@google.com>",
  "repository": "git@github.com:google/wikiloop-doublecheck.git",
  "scripts": {
    "build:analyze": "nuxt build -a",
    "build": "nuxt build",
    "ci": "circleci local execute build",
    "ci:validate": "circleci config validate",
    "dev:debug": "cross-env NODE_ENV=development nodemon --inspect",
    "dev:exec": "ts-node -r tsconfig-paths/register --project tsconfig.json ./server/index.ts",
    "dev:lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "dev:precommit": "npm run lint",
    "dev:server": "nodemon -w server -w shared -x ts-node -r tsconfig-paths/register ./server/index.ts --server-only",
    "dev:translate": "npx ts-node -r tsconfig-paths/register tscmd/translate.ts",
    "dev": "cross-env NODE_ENV=development nodemon",
    "heroku-postbuild": "npm run build",
    "prod:pm2": "npx pm2 start eco.yml --env=production",
    "prod": "npm run build && npm run start",
    "release": "HUSKY_SKIP_HOOKS=1 standard-version",
    "sb": "nuxt storybook",
    "build-storybook": "nuxt storybook build",
    "start": "npm run dev",
    "test:integration": "jest --testPathIgnorePatterns=THIS_SHALL_NEVER_MATCH_ANYTHING --testPathPattern=integration --verbose",
    "test:full": "jest --testPathIgnorePatterns=THIS_SHALL_NEVER_MATCH_ANYTHING --verbose",
    "test:report": "jest --testPathIgnorePatterns=THIS_SHALL_NEVER_MATCH_ANYTHING --verbose --coverage --ci",
    "test": "jest --verbose --bail",
    "chromatic": "chromatic --project-token=${CHROMATIC_PROJECT_TOKEN}"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn dev:lint && yarn test",
      "pre-push": "yarn test:full",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "dependencies": {
    "@babel/core": "^7.12.9",
    "@dicebear/avatars": "^4.4.1",
    "@dicebear/avatars-gridy-sprites": "^4.4.1",
